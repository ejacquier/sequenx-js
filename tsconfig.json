{
    "formatCodeOptions": {
        "indentSize": 4,
        "tabSize": 4,
        "newLineCharacter": "\r\n",
        "convertTabsToSpaces": true,
        "insertSpaceAfterCommaDelimiter": true,
        "insertSpaceAfterSemicolonInForStatements": true,
        "insertSpaceBeforeAndAfterBinaryOperators": true,
        "insertSpaceAfterKeywordsInControlFlowStatements": true,
        "insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
        "insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": false,
        "placeOpenBraceOnNewLineForFunctions": true,
        "placeOpenBraceOnNewLineForControlBlocks": true
    },
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "outFile": "js/sequenx.js",
        "sourceMap": true
    },
    "filesGlob": [
        "./**/*.ts",
        "!node_modules/**/*.ts"
    ],
    "files": [
        "./src/sequencing/ICompletable.ts",
        "./src/sequencing/ILapse.ts",
        "./src/sequencing/impl/Lapse.ts",
        "./src/sequencing/impl/Sequence.ts",
        "./src/sequencing/IParallel.ts",
        "./src/sequencing/ISequence.ts",
        "./typings/rx.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true,
        "formatOnSave": true
    },
    "buildOnSave": false,
    "compileOnSave": false
}
